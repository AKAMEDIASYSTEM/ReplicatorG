<?xml version="1.1" encoding="utf-8"?>
<machines>
	<!-- AKA Zen toolworks machine file, modded from Cupcake Basic -->
	<machine>
		<name>AKA_7x7</name>
		<geometry type="cartesian">
			<!-- different pulleys on X and Y axii -->
			<axis id="x" length="176" maxfeedrate="400" stepspermm="160" endstops="min"/> 
			<axis id="y" length="176" maxfeedrate="400" stepspermm="160" endstops="min"/>
			<axis id="z" length="48" maxfeedrate="200" stepspermm="320" endstops="min"/>
		</geometry>
		<tools>
			<tool name="Pinch Wheel Extruder v1.1" type="extruder" material="abs" motor="true" fan="true" heater="true"/>
		</tools>
		<clamps></clamps>
		<driver name="sanguino3g">
			<!-- optional: <portname>COM1</portname> -->
			<rate>38400</rate>
		</driver>
		<warmup>
		</warmup>
		<cooldown>
(Turn off steppers after a build.)
M18
		</cooldown>
	</machine>
